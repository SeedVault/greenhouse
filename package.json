{
  "name": "greenhouse",
  "version": "1.0.0",
  "description": "Node.js REST API boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel src --out-dir dist",
    "clean": "rimraf dist",
    "debug": "DEBUG=greenhouse.*,express:* nodemon --watch .env --watch src --exec babel-node src/bin/www.js --source-maps",
    "dev": "NODE_ENV=development nodemon --watch .env --watch src --exec babel-node src/bin/www.js --source-maps",
    "lint": "eslint src",
    "prod": "npm run build && NODE_ENV=production node ./dist/bin/www",
    "start": "node dist",
    "test": "jest --runInBand --watchAll",
    "nohelp": "nodemon --help config"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:SeedVault/greenhouse.git"
  },
  "keywords": [
    "api",
    "express",
    "rest"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^3.4.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "csurf": "^1.10.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "helmet": "^3.20.0",
    "http-errors": "^1.7.3",
    "https-proxy-agent": "^2.2.2",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.5.0",
    "passport-oauth2-refresh": "^1.1.0",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/runtime": "^7.5.4",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2"
  }
}
