{
  "name": "@seed-vault/greenhouse",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:SeedVault/greenhouse.git"
  },
  "scripts": {
    "build": "vue-cli-service build && rm -Rf ./dist/uploads/ && cd ./dist && ln -s ../public/uploads uploads && cd ..",
    "debug": "DEBUG=*:* nodemon --exec 'vue-cli-service serve'",
    "export": "NODE_ENV=production node -r dotenv/config ./scripts/ExportAll",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "lint": "vue-cli-service lint",
    "prod": "NODE_ENV=production node -r dotenv/config ./server/bin/www.js",
    "prod:localhost": "NODE_ENV=production node -r dotenv/config ./server/bin/wwws.js",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit --runInBand --detectOpenHandles --useStderr --forceExit",
    "serve": "DEBUG=greenhouse:*,mongoose:* nodemon --exec 'vue-cli-service serve'"
  },
  "dependencies": {
    "@seed-vault/seedtoken-api-client": "github:SeedVault/seedtoken-api-client",
    "axios": "^0.19.0",
    "bignumber.js": "^9.0.0",
    "body-parser": "^1.19.0",
    "bootstrap-vue": "^2.0.4",
    "chart.js": "^2.9.3",
    "connect-redis": "^4.0.3",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.3.2",
    "csurf": "^1.10.0",
    "ejs": "^2.7.2",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "moment": "^2.24.0",
    "mongoose": "^5.7.11",
    "mongoose-unique-validator": "^2.0.3",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-seed-accounts": "file:../passport-seed-accounts",
    "redis": "^2.8.0",
    "saslprep": "^1.0.3",
    "seed-theme": "file:../seed-theme",
    "uuid4": "^1.1.4",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.5.0",
    "vue-i18n": "^8.15.0",
    "vue-router": "^3.1.3",
    "vue-star-rating": "^1.6.1",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.4.1",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-e2e-cypress": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "nodemon": "^1.19.4",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-template-compiler": "^2.6.10"
  }
}
